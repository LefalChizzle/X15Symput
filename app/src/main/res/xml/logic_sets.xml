<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:keyHeight="@dimen/key_height"
    android:verticalGap="0dp">

    <Row>
        <Key
            android:codes="49"
            android:keyEdgeFlags="left"
            android:keyLabel="1" />
        <Key
            android:codes="50"
            android:keyLabel="2" />
        <Key
            android:codes="51"
            android:keyLabel="3" />
        <Key
            android:codes="52"
            android:keyLabel="4" />
        <Key
            android:codes="53"
            android:keyLabel="5" />
        <Key
            android:codes="54"
            android:keyLabel="6" />
        <Key
            android:codes="55"
            android:keyLabel="7" />
        <Key
            android:codes="56"
            android:keyLabel="8" />
        <Key
            android:codes="57"
            android:keyLabel="9" />
        <Key
            android:codes="48"
            android:keyEdgeFlags="right"
            android:keyLabel="0" />
    </Row>
    <!--First Page for Math -->
    <Row>
        <Key
            android:codes="8709"
            android:keyEdgeFlags="left"
            android:keyLabel="∅"
            android:popupKeyboard="@xml/popup"
            app:subLabel="+" />
        <Key
            android:codes="8469"
            android:keyLabel="ℕ"
            app:subLabel="-"
            />
        <Key
            android:codes="8484"
            android:keyLabel="ℤ"
            app:subLabel="∙"
            />
        <Key
            android:codes="8474"
            android:keyLabel="ℚ"
            app:subLabel="÷"
            />
        <Key
            android:codes="8450"
            android:keyLabel="ℂ"
            app:subLabel="="
            />
        <Key
            android:codes="8477"
            android:keyLabel="ℝ"
            app:subLabel="&lt;"
             />
        <Key
            android:codes="8712"
            android:keyLabel="∈"
            app:subLabel="&gt;"
             />
        <Key
            android:codes="8838"
            android:keyLabel="⊆"
            app:subLabel="≤"
             />
        <Key
            android:codes="8746"
            android:keyLabel="∪"
            app:subLabel="≥"
             />
        <Key
            android:codes="8745"
            android:keyEdgeFlags="right"
            android:keyLabel="∩"
             />
    </Row>

    <Row>
        <Key
            android:codes="8721"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="∑"
             />
        <Key
            android:codes="124"
            android:keyLabel="|"
             />
        <Key
            android:codes="8707"
            android:keyLabel="∃"
             />
        <Key
            android:codes="8704"
            android:keyLabel="∀"
             />
        <Key
            android:codes="172"
            android:keyLabel="¬"
             />
        <Key
            android:codes="8896"
            android:keyLabel="∧"
             />
        <Key
            android:codes="8744"
            android:keyLabel="∨"
             />
        <Key
            android:codes="8869"
            android:keyLabel="⊥"
             />
        <Key
            android:codes="8868"
            android:keyEdgeFlags="right"
            android:keyLabel="⊤"
             />
    </Row>

    <Row>
        <Key
            android:codes="@integer/keycode_shift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ic_keyboard_arrow_up"
            android:keyWidth="15%p" />
        <Key
            android:codes="8594"
            android:keyLabel="→"
            />
        <Key
            android:codes="1013"
            android:keyLabel="ϵ" />
        <Key
            android:codes="8800"
            android:keyLabel="≠" />
        <Key
            android:codes="8756"
            android:keyLabel="∴" />
        <Key
            android:codes="8757"
            android:keyLabel="∵"
            />
        <Key
            android:codes="8759"
            android:keyLabel="∷"
            />
        <Key
            android:codes="61"
            android:keyLabel="=" />
        <Key
            android:codes="@integer/keycode_delete"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_outline_backspace"
            android:keyWidth="15%p" />
    </Row>

    <Row
        android:keyHeight="@dimen/bottom_keys_height"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/keycode_abc"
            android:keyEdgeFlags="left"
            android:keyLabel="\?123"
            android:keyWidth="15%p"
            app:keyTextSize="6dp" />
        <Key
            android:codes="@integer/keycode_switch_next_keyboard"
            android:keyIcon="@drawable/ic_language"
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:keyWidth="50%p"
            app:isChangeLanguageKey="true"
            app:keyTextSize="6dp" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p" />
        <Key
            android:codes="@integer/keycode_done"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_keyboard_return"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>